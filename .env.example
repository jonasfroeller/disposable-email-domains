# Single or multiple admin tokens (tokens shorter than 16 chars are ignored). If ADMIN_TOKENS is set it supersedes ADMIN_TOKEN.
# ADMIN_TOKEN=your_single_admin_token_value_here
# ADMIN_TOKENS=token_one_aaaaaaaaaaaaaaaa,token_two_bbbbbbbbbbbbbbbb

# Rate limiting
# RATE_LIMIT_RPS=5
# RATE_LIMIT_BURST=20
# RATE_LIMIT_BUCKET_TTL=10m
# RATE_LIMIT_BYPASS_DOMAINS=42websites.com

# Public Suffix List refresher
# PSL_REFRESH_INTERVAL=24h

# Background sample warming (disabled by default)
# ENABLE_SAMPLE_WARMING=false
# SAMPLE_CHECK_INTERVAL=10m

# Trust reverse proxy headers for client IP (X-Forwarded-For / X-Real-IP)
# TRUST_PROXY_HEADERS=false

# Server port
# PORT=8080
